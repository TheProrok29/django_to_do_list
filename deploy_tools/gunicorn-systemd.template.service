[Unit]
Description=Gunicorn server for SITENAME

[Service]
Restart=on-failure
User=prorok
WorkingDirectory=/home/prorok/sites/SITENAME/source
ExecStart=/home/prorok/sites/SITENAME/virtualenv/bin/gunicorn --bind unix:/tmp/SITENAME.socket superlists.wsgi:application

[Install]
WantedBy=multi-user.target